       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.1.xsd">

    <bean id="sessionBean"
          class="com.yaroma.SessionBean"/>
            
    <bean id="userService"
          class="com.yaroma.equilibrium.service.implementation.UserServiceImplementation"/>
    
    <bean id="customerService"
          class="com.yaroma.equilibrium.service.implementation.CustomerServiceImplementation"/>
    
    <bean id="softwareService"
          class="com.yaroma.equilibrium.service.implementation.SoftwareServiceImplementation"/>

    <bean id="linkService"
              class="com.yaroma.equilibrium.service.implementation.LinkServiceImplementation"/> 
           
    <bean id="statService"
          class="com.yaroma.equilibrium.service.implementation.StatServiceImplementation"/>
   

</beans>